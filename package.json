{
    "name": "nestjs-broilerplate",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest --config=jest.config.ts",
        "test:watch": "jest --config=jest.config.ts --watch",
        "test:cov": "jest --config=jest.config.ts --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "ts-node ./node_modules/typeorm/cli",
        "migration:run": "npm run typeorm migration:run -- -d ./src/config/database.config.ts",
        "migration:generate": "npm run typeorm -- migration:generate -d ./src/config/database.config.ts -p ./src/migrations/%npm_config_name%",
        "migration:create": "npm run typeorm -- migration:create ./src/migrations/%npm_config_name% ",
        "migration:revert": "npm run typeorm -- -d ./src/config/database.config.ts migration:revert",
        "seed": "ts-node -r tsconfig-paths/register src/seed/role.seed.ts"
    },
    "dependencies": {
        "@keyv/redis": "^4.4.0",
        "@nest-lab/throttler-storage-redis": "^1.1.0",
        "@nestjs/cache-manager": "^3.0.1",
        "@nestjs/common": "^11.0.1",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.0.1",
        "@nestjs/event-emitter": "^3.0.1",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/platform-express": "^11.0.1",
        "@nestjs/swagger": "^11.1.5",
        "@nestjs/throttler": "^6.4.0",
        "@nestjs/typeorm": "^11.0.0",
        "bcrypt": "^5.1.1",
        "cache-manager": "^6.4.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "dotenv": "^16.5.0",
        "glob": "^11.0.2",
        "ioredis": "^5.6.1",
        "lodash": "^4.17.21",
        "nest-winston": "^1.10.2",
        "pg": "^8.15.6",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.18.0",
        "@nestjs/cli": "^11.0.0",
        "@nestjs/schematics": "^11.0.0",
        "@nestjs/testing": "^11.1.0",
        "@suites/di.nestjs": "^3.0.1",
        "@suites/doubles.jest": "^3.0.1",
        "@suites/unit": "^3.0.1",
        "@swc/cli": "^0.6.0",
        "@swc/core": "^1.10.7",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.15.2",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^8.33.1",
        "@typescript-eslint/parser": "^8.33.1",
        "eslint": "^9.28.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.2.2",
        "globals": "^16.0.0",
        "jest": "^29.7.0",
        "prettier": "^3.5.3",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.3.2",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typeorm": "^0.3.22",
        "typeorm-ts-node-commonjs": "^0.3.20",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.20.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
